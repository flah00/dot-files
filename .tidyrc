doctype: omit
char-encoding: utf8
literal-attributes: yes
fix-uri: no
#preserve-phil: yes
show-body-only: auto
indent-cdata: yes
indent: yes
indent-spaces: 2
wrap: 0
markup: yes
output-xml: no
input-xml: no
show-warnings: yes
numeric-entities: yes
quote-marks: no
quote-nbsp: yes
quote-ampersand: no
break-before-br: no
uppercase-tags: no
uppercase-attributes: no
char-encoding: latin1
new-blocklevel-tags: td
merge-divs: no
merge-spans: no
output-file: /tmp/buffer.html
error-file: /tmp/buffer.log
